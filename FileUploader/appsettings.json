{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TranscodeOption": {
    "FFmpegPath": "ffmpeg.exe",
    "FFprobePath": "ffprobe.exe",
    "OutputDirectory": "Uploads",
    "SegmentDuration": 15,
    "DefaultFrameRate": "24",
    "DefaultThreadCount": 4,
    "DefaultAudioCodec": "aac",
    "DefaultVideoCodec": "libx264",
    "DefaultPreset":  "ultrafast"
  }
}
